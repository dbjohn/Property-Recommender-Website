
.center{
	text-align:left;
}

#table{
	width:100%;
	max-width:1600px;
}

#slider{	
	width:660px;	
}

#range th{
   /*background-image:url(<%= asset_path 'th.png' %>);*/
   height:50px;      
   color: white;
   text-shadow: #012b4d 2px 2px 2px;
   text-align: center;   
   background-color: #73AEE9;    
   border: 1px solid black;
}

#range td{
	text-indent:5px;
	color:#444;	
	border:1px solid black;	/*jb modified so that border is all around cells*/
	text-align: center;
}

td.left{
	border-left:1px solid #2e638e;
}

td.right{
	border-right:1px solid #2e638e;
}

td.bottom{
	border-bottom:1px solid #2e638e;
}

.grip{
	width:20px;
	height:30px;
	margin-top:-3px;
	background-image:url(<%= asset_path 'grip.png' %>);
	margin-left:-5px;
	position:relative;
	z-index:88;
	cursor:e-resize;
}

.grip:hover{
	background-position-x:-20px;
}

.dragging .grip{
	background-position-x:-40px;
}

.sampleText{
	position:relative;
	width:100%;
}

.dotted{
	background-image:url(<%= asset_path 'dotted.png' %>);
	background-repeat:repeat-y;
}

